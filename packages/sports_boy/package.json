{
  "name": "sports_boy",
  "version": "1.0.0",
  "main": "./dist/sports_boy.umd.js",
  "module": "./dist/sports_boy.es.js",
  "typings": "./dist/src/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build:theme": "node scripts/autoBuildIndexScss.js & npx sass  --no-source-map src/theme:dist/lib/theme",
    "build:theme:watch": "npx sass --watch  --no-source-map src/theme:dist/lib/theme ",
    "build": "vue-tsc --noEmit && vite build && yarn build:theme",
    "build:watch": "vue-tsc --noEmit && build=watch vite build && yarn build:theme:watch",
    "serve": "vite preview"
  },
  "files": [
    "dist",
    "src",
    "types",
    "README.md"
  ],
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@babel/runtime-corejs3": "^7.16.3",
    "@types/node": "^16.11.10",
    "@vitejs/plugin-vue-jsx": "^1.3.0",
    "scss": "^0.2.4",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-dts": "^0.9.6",
    "vue-tsc": "^0.3.0"
  }
}
