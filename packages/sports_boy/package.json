{
  "name": "sports_boy",
  "version": "1.0.0",
  "main": "./dist/sports_boy.umd.js",
  "module": "./dist/sports_boy.es.js",
  "typings": "./dist/src/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build:theme": "node scripts/autoBuildIndexScss.js & gulp build",
    "build:theme:watch": "node scripts/autoBuildIndexScss.js & gulp watch",
    "build": "vue-tsc --noEmit && vite build && yarn build:theme",
    "build:watch": "vue-tsc --noEmit && cross-env build=watch vite build",
    "serve": "vite preview"
  },
  "files": [
    "dist",
    "src",
    "types",
    "README.md"
  ],
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@babel/runtime-corejs3": "^7.16.3",
    "@types/node": "^16.11.10",
    "@vitejs/plugin-vue-jsx": "^1.3.0",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.12",
    "gulp": "^4.0.2",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "cross-env": "^7.0.3",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.0.0",
    "postcss": "^8.4.4",
    "sass": "^1.45.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-dts": "^0.9.6",
    "vue-tsc": "^0.3.0"
  }
}
